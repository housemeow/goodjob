{
  "manifest_version": 2,
  "name": "goodjob",
  "version": "0.0.1",
  "description": "Kellly wants a good job.",
  "icons" :{
    "16": "onelook.png"
  },
  "author": "kellly",
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background":{
    "scripts": ["goodjob.js", "background.js"]
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}